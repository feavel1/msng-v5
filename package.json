{
  "name": "svelte5",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "paraglide-js compile --project ./project.inlang && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "eslint .",
    "postinstall": "paraglide-js compile --project ./project.inlang"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "1.2.5",
    "@inlang/paraglide-js-adapter-sveltekit": "^0.4.1",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/adapter-cloudflare": "^4.1.0",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/eslint": "8.56.2",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.36.0-next.4",
    "svelte": "^5.0.0-next.53",
    "svelte-check": "^3.6.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "type": "module",
  "packageManager": "yarn@1.22.19"
}
